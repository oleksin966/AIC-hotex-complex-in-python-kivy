<LoginPage>:
	name: 'login_page'
	BoxLayout:
		orientation: 'vertical'
		pos_hint: {'center_x':0.5, 'center_y':0.9}
		spacing: 0
		padding: [0,400,0,200]
		MDLabel:
			text: 'АIC Готельного Комплексу'
			halign: 'center'
			font_size: 36
		Image:
			source: 'hotel.png'
			pos_hint: {'center_x':0.5, 'center_y':0.9}
	BoxLayout:
		orientation: 'vertical'
		padding: [56,400,56,400]
		spacing: 20
		#size: 100,200
		pos_hint: {'center_x':0.5, 'center_y':0.5}
		TextInput:
			id: login
			size_hint: (None, None)
			height: 50
			width: 300
			multiline:False
			font_size: 28
			hint_text: "Логін"
		TextInput:
			id: passw
			size_hint: (None, None)
			height: 50
			width: 300
			multiline:False
			password:True
			font_size: 28
			hint_text: "Пароль"
		Button:
			size_hint: (None, None)
			height: 50
			width: 300
			text: "Вхід"
			font_size: 24
			on_release: root.verify_credentials()



<MainTitleExit>
	BoxLayout:
		orientation: 'horizontal'
		size_hint:(None, None)
		width: root.width
		height: "55dp"
		pos: 0, root.height - 55
		#pos_hint: {'top':0.5}
		BoxLayout:
			canvas:
				Color:
					rgba: 0.5, 0.5, 0.5, 0.5
				Rectangle: 
					size: self.size
					pos: self.pos
			orientation: 'horizontal'
			padding: [0, 0, 100, 0]
			#spacing: 70
			Button:
				id: btn_menu
				size_hint: 1, 1
				text: '<< Вихід'
				size: 50, 40
				font_size: 24
				on_release: root.exit()
		BoxLayout:
			canvas:
				Color:
					rgba: 0.5, 0.5, 0.5, 0.5
				Rectangle: 
					size: self.size
					pos: self.pos
			orientation: 'horizontal'
			padding: [0, 0, 100, 0]
			#spacing: 70
			Label:
				id: date
				text: '18 листопада 2022'
				font_size: 20
				color: [0,0,0]






<MenuScreen>:
	name: 'menu'   
	BoxLayout:
		pos_hint: {'top': .95}
		ScrollView:
			MDList:
				ThreeLineListItem:
					text: "1.Перелік і загальне число фірм, що забронювали"
					secondary_text: "місця в обсязі, не менше вказаного,"
					tertiary_text: "за весь період співпраці, або за деякий період."
					on_release: root.manager.current = "first_info"
				ThreeLineListItem:
					text: "2.Перелік і загальне число постояльців,"
					secondary_text:"що заселяли в номери із зазначеними " 
					tertiary_text:"характеристиками за певний період."
					on_release: root.manager.current = "second_info"
				OneLineListItem:
					text : "3.Кількість вільних номерів на даний момент."
					on_release: root.manager.current = "third_info"
				TwoLineListItem
					text : "4.Відомості про кількість вільних номерів" 
					secondary_text:"в із зазначеними характеристиками."
					on_release: root.manager.current = "four_info"
				ThreeLineListItem:
					text : "5.Відомості по конкретному вільному номеру:"
					secondary_text:"протягом якого часу він буде пустувати"
					tertiary_text:"і про його характеристики."
					on_release: root.manager.current = "five_info"
				TwoLineListItem:
					text : "6.Список зайнятих зараз номерів, котрі"
					secondary_text:"звільняються до зазначеного терміну."
					on_release: root.manager.current = "six_info"
				ThreeLineListItem:
					text : "7.Дані про обсяг бронювання номерів" 
					secondary_text:"даною фірмою за вказаний період,"
					tertiary_text:"і якими номерами віддавалися переваги."
					on_release: root.manager.current = "seven_info"
				OneLineListItem:
					text : "8.Список незадоволених клієнтів та їхні скарги."
					on_release: root.manager.current = "eight_info"
				OneLineListItem:
					text : "9.Інформація про доходи"
					on_release: root.manager.current = "nine_info"
				ThreeLineListItem:
					text : "10.Відомості про конкретних осіб та фірм" 
					secondary_text:"які більше за всіх відвідують "
					tertiary_text:"готель по всім корпусам."
					on_release: root.manager.current = "eleven_info"
				OneLineListItem:
					text : "11.Відомості про нових клієнтів за вказаний період."
					on_release: root.manager.current = "twelve_info"
				ThreeLineListItem:
					text : "12.Відомості про конкретну людину скільки разів " 
					secondary_text:"він відвідував готель, в яких номерах і в який" 
					tertiary_text:"період зупинявся, які рахунки оплачував."
					on_release: root.manager.current = "thirteen_info"
				TwoLineListItem:
					text : "13.Відомості про конкретний номер:" 
					secondary_text:"ким він був зайнятий в певний період."
					on_release: root.manager.current = "fourteen_info"
				TwoLineListItem:
					text : "14.Відсоткове відношення всіх номерів до номерів" 
					secondary_text:"заброньованих партнерами"
					on_release: root.manager.current = "fifteen_info"
				TwoLineListItem:
					text : "" 
					secondary_text:""
	MainTitleExit:










